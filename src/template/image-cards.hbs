<ul class="card-set">
{{#each this}}
<li class="item">
  <article class="card">

  <div class="card-thumb">
      <a class="card-thumb__item" href="{{largeImageURL}}">
        <img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" />
      </a>
  </div>

  <div class="card-info">
    <p class="info-item">
     <b>Likes</b>
      <b>{{likes}}</b>
    </p>
    <p class="info-item">
      <b>Views</b>
      <b>{{views}}</b>
    </p>
    <p class="info-item">
      <b>Comments</b>
      <b>{{comments}}</b>
    </p>
    <p class="info-item">
      <b>Dowloads</b>
      <b>{{downloads}}</b>
    </p>
  </div>

</article>
</li>
{{/each}}
</ul>
